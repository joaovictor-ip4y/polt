{
  "name": "joaovictor/polt",
  "description": "Projeto de exemplo para CI/CD com PHP e Docker",
  "require": {
      "php": "^8.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.6"
  },
  "autoload": {
      "psr-4": {
          "App\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "src/tests/"
      }
  },
  "scripts": {
      "test": "vendor/bin/phpunit"
  }
}
